{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "amardatafctoryproject"
		},
		"LSONPREMfile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LSONPREMfile'"
		},
		"ls_azure_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"LSONPREMfile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\amarkishorefiles"
		},
		"LSONPREMfile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "rajashreesenapati4@gmail.com"
		},
		"ls_azure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfprojectamar.database.windows.net"
		},
		"ls_azure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfprojectdb"
		},
		"ls_azure_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "amaradmin"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfamarstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "amardatafctoryproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_password": {
						"value": "[parameters('ls_azure_password')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"LSONPREMfile_password": {
						"value": "[parameters('LSONPREMfile_password')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"ls_azure_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_properties_typeProperties_server')]"
					},
					"ls_azure_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_properties_typeProperties_database')]"
					},
					"ls_azure_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"LSONPREMfile_properties_typeProperties_host": {
						"value": "[parameters('LSONPREMfile_properties_typeProperties_host')]"
					},
					"LSONPREMfile_properties_typeProperties_userId": {
						"value": "[parameters('LSONPREMfile_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "amardatafctoryproject_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'amardatafctoryproject_ArmTemplate_0')]"
			]
		}
	]
}